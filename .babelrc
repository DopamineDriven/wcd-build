{
	"env": {
		"development": {
			"plugins": [
				[
					"styled-components",
					{ "ssr": true, "displayName": true, "preprocess": false }
				],
				[
					"import",
					{
						"libraryName": "antd",
						"libraryDirectory": "lib",
						"style": "index.css"
					}
				],
				[
					"import",
					{
						"libraryName": "@ant-design/icons",
						"libraryDirectory": "lib/icons",
						"camel2DashComponentName": false
					},
					"@ant-design/icons"
				]
			],
			"presets": ["next/babel"]
		},
		"production": {
			"plugins": [
				[
					"styled-components",
					{ "ssr": true, "displayName": true, "preprocess": false }
				],
				[
					"import",
					{
						"libraryName": "antd",
						"libraryDirectory": "lib",
						"style": "index.css"
					}
				],
				[
					"import",
					{
						"libraryName": "@ant-design/icons",
						"libraryDirectory": "lib/icons",
						"camel2DashComponentName": false
					},
					"@ant-design/icons"
				]
			],
			"presets": ["next/babel"]
		}
	},
	"plugins": [
		[
			"styled-components",
			{ "ssr": true, "displayName": true, "preprocess": false }
		],
		[
			"import",
			{
				"libraryName": "antd",
				"libraryDirectory": "lib",
				"style": "index.css"
			}
		],
		[
			"import",
			{
				"libraryName": "@ant-design/icons",
				"libraryDirectory": "lib/icons",
				"camel2DashComponentName": false
			},
			"@ant-design/icons"
		]
	]
}
